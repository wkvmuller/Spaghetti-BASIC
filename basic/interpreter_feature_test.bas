10 DATA 1,2,3,4
20 DIM A(2,2)
30 MAT READ A
40 MAT PRINT A
50 MAT B = A * 2
60 MAT PRINT B
70 FOR I = 0 TO 2
80   PRINT "FOR LOOP I = ", I
90 NEXT I
100 WHILE A(0) < 10
110   PRINT "WHILE LOOP, A(0) = ", A(0)
120   LET A(0) = A(0) + 1
130 WEND
140 DO
150   PRINT "DO LOOP"
160   LET A(1) = A(1) + 1
170 LOOP UNTIL A(1) > 5
180 PRINT "RND() = ", RND()
190 SEED(1234)
200 PRINT "TIME() = ", TIME()
210 LET X = SIN(45)
220 PRINT "SIN(45) = ", X
230 LET Y$ = MID$("HELLO", 1, 3)
240 PRINT "MID$ = ", Y$
250 END
