10 REM === BASIC EXPRESSIONS & ASSIGNMENTS ===
20 LET A = 10
30 LET B = 3
40 LET C = A + B * 2
50 PRINT "C=", C

60 REM === TRIG MODE AND FUNCTIONS ===
70 DEG
80 LET D1 = SIN(30)
90 LET D2 = COS(60)
100 RAD
110 LET D3 = TAN(1)
120 LET D4 = ASIN(0.5)
130 LET D5 = ACOS(0.5)
140 LET D6 = ATAN(1)
150 PRINT "SIN(30)=", D1, "COS(60)=", D2, "TAN(1 rad)=", D3
