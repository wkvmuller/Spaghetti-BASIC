170 REM === LOG / EXP / POW / CONVERSION FUNCTIONS ===
180 LET L1 = LOG(2.718)
190 LET L2 = LOG10(100)
200 LET E1 = EXP(1)
210 LET P1 = POW(2, 5)
220 LET C1 = RAD2DEG(3.14159)
230 LET C2 = DEG2RAD(180)
240 PRINT "LOG=", L1, "LOG10=", L2, "EXP=", E1
250 PRINT "2^5=", P1, "RAD2DEG(PI)=", C1, "DEG2RAD(180)=", C2

260 REM === INPUT / TRACE / DEBUG / BREAK ===
270 INPUT X
280 DEBUG
290 LET Y = X * 10
300 TRACE Y
310 BREAK
320 NODEBUG

330 REM === DATA / READ / RESTORE ===
340 DATA 100, 200, 300
350 READ D7, D8
360 PRINT "DATA READ:", D7, D8
370 RESTORE
380 READ D9
390 PRINT "DATA RESTORED:", D9

400 REM === DEF FN and FUNCTION CALL ===
410 DEF FN SQR(X) = POW(X, 0.5)
420 LET SQ = FN SQR(81)
430 PRINT "SQRT(81)=", SQ

